                                                <div class="section" data-plugin="comment-user-badge" data-name="{{:name}}">
                                                    <img data-src="{{:image}}" title="{{:name}}"  src="/photo/49258704.cms">
                                                    <span class="name">{{:name}}</span>
                                                    <span class="badgepoint">{{:count}}</span>
                                                </div>
                                            {{/for}}
                                           <!-- / badges End here -->
                                           
                                           <div class="more-info">
                                           <a href="https://timesofindia.indiatimes.com/abouttimesrewards.cms" class="more" target="_blank">Know more about Times Points</a>
                                           </div>
                                       </div>
                                    </div>
                                </div>
                             {{/if}}
                        </div>
                    {{/if}}
                </div>
                {{if parentusername}}
