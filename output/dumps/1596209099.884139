                {{/if}}
            </div>
            <div class="info-bar clearfix">
                {{if user.username || user.id}}
                    <a href="https://mytimes.indiatimes.com/profile/{{:user.username || user.id}}" class="name" target="_blank">{{:user.name}}</a>
                {{else}}
                    <span class="name">{{:user.name}}</span>
                {{/if}}
                <div class="points_wrap">
                    {{if user.points}}
