                var chartData = eval(Datastring[0]);
                var PrevClose = Datastring[1];
                var chart = AmCharts.makeChart('divCurrencyChartNew', { 'type': 'serial', 'theme': 'light', 'paddingRight': 0, 'color': '#fff', 'fontfamily': 'Roboto Condensed',
                    'pathToImages': '/images/',
                    'dataProvider': chartData,
                    'valueAxes': [{ 'id': 'v1',
