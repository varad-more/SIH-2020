            biCommonUtil.loadComscore = function loadComscore(){
                var comscoreScriptTag = document.createElement("script"), 
                firstScriptTag = document.getElementsByTagName("script")[0]; 
                comscoreScriptTag.async = true;
                comscoreScriptTag.onload = function(){
                    // Trigger comscore call on beacon load
