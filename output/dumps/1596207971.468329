                firstScriptTag.parentNode.insertBefore(comscoreScriptTag, firstScriptTag);
            }
            biCommonUtil.fireComscore = function fireComscore() {
                if (typeof(COMSCORE) !== "undefined" && typeof(COMSCORE.beacon) === "function") {
                    // Triggering both calls India & Global
                    COMSCORE.beacon(objComScoreIndia);
                    COMSCORE.beacon(objComScoreGlobal);
                }else{
                    console.log("Beacon load failed!!");
                }
            }
            var initiatePVCAjaxCall = function(isLoadMoreCall=false, $selector){
                if(isLoadMoreCall){
                    var isPVCFired = $selector.data("pageview_candidate_fired");
                    if (isPVCFired != "yes" && isPVCFired != "inprogress") {
                        $selector.data("pageview_candidate_fired", "inprogress");
                        $.ajax({
                            url: "/pageview_candidate.cms",
                            cache: false,
                            dataType: "xml",
                            success: function(c) {
                                $selector.data("pageview_candidate_fired", "yes")
                            },
                            error: function(c) {
                                $selector.data("pageview_candidate_fired", "error")
                            }
                        });
                    }
                }else{
                    $.ajax({
                        url: "/pageview_candidate.cms",
                        cache: false,
                        dataType: "xml",
                        success: function(c) {
                            console.info("Pageview candidate success");
                        },
                        error: function(c) {
                            console.error("Pageview candidate call failed");
                        }
                    });
                }
            }
            biCommonUtil.firePageViewCandidate = function(slideDetails, isElement, parentSlide) {
                if(slideDetails){
                    var triggerFlag;
                    if(parentSlide){
                        var $selector = $('[data-article="list'+slideDetails+'"]')
                        var currentSlidelist = Number($selector.data("article").split("list")[1]);
                        if(!isNaN(currentSlidelist) && (currentSlidelist != parentSlide)){
                            triggerFlag = true;
                        }else{
                            triggerFlag = false;
                        }
                    }else{
                        var $selector = isElement ? slideDetails : $("[data-slide-id=" + slideDetails + "]");
                        triggerFlag = true;
                    }
                    if(triggerFlag){
                        initiatePVCAjaxCall(true, $selector);
                    }
                }else{
                    <!--For load more on listing pages-->
                    initiatePVCAjaxCall();
                }
            }
            if(typeof TimesGDPR != 'undefined' && typeof TimesGDPR.common.consentModule.gdprCallback == 'function'){
                TimesGDPR.common.consentModule.gdprCallback(function(dataObj){
                        if (!!dataObj.isEUuser){
                            objComScoreIndia['cs_ucfr'] = 0;
                            objComScoreGlobal['cs_ucfr'] = 0;
                        }
                        biCommonUtil.loadComscore();
                });
            }
