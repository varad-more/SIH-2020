		function ajaxCall(feedurl,section){
			$.ajax({
				url: feedurl,
				dataType: 'json',
				success: function (data) {
					if(data.content.sectionItems.length>0){
					GenerateData(data, section,true);
					}
				},
				error: function (jqXHR, exception) {
					var smartIp = JSON.parse(localStorage.getItem('obj' + section));
					GenerateData(smartIp, section,false);
				}
			});
		}
		
		function GenerateData(data,id,setDateTime){
			var str='';
			if(setDateTime){
				var dt = new Date();
		        dt.setMinutes( dt.getMinutes() + 15 ); 
				var object = {response : data, timestamp: dt.getTime()}
				localStorage.setItem('obj'+id, JSON.stringify(object));
			}
		
		var webtype=data.content.webtype;
		var url=data.content.sectionUrl;
		if(!isEndWithSlash(url)){
			url=url+'/';
		}
		str +='<div class="column-head"><div class="new-h2-head"><a title="'+ data.content.sectionName +'" href="'+url+'"><span class="color-bx all-categories-bar"></span>'+data.content.sectionName+'</a></div></div>';
		str +='<div class="row"><div class="col-sm-12 col-md-12">';
		str +='<ul class="latest-news-bx new-bg-bx">';
		$.each( data.content.sectionItems, function( i, item)
		{
			if(i<4){
			str+='<li>';
			str+='<div class="media">';
			str+='<div class="media-left">';
			str+='<div class="media-img">';
			str+='<a href='+item.websiteURL+'><img class="lazy" src="https://www.hindustantimes.com/images/app-images/2019/7/Placeholder_90x90.png" title="' + item.headLine + '" alt="' + item.headLine + '" data-src="'+item.thumbImage+'"/></a>';
			str+='</div>';
			str+='</div>';
			str+='<div class="media-body">';
			str+='<div class="media-heading headingfive"><a title="'+ item.headLine +'" href="'+item.websiteURL+'">'+item.headLine+'</a></div>';
			str+='</div>';
			str+='</div>';
			str+='</li>';
			}
		});
		str +='</ul>';
		str +='</div></div>';		
		$("#"+id).html(str);
		$("#"+id + " img.lazy").lazy({ effect : "fadeIn", effectTime : 20, threshold : 200, failurelimit : 0 });
		}
		
		function isEndWithSlash(str){
			var strlen=str.length;
			var startIndex=strlen-1;
			var lastChar=str.substr(startIndex,strlen);
			if(lastChar==='/'){
				return true;
			}else{
				return false;
			}
		}
		</script>
