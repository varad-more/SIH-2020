    	    		var matchUrl = "https://www.hindustantimes.com/sticky-feed/icc-world-cup-2019";
    	    		isLive=false;
    	    		$.ajax({
    	    			url:matchUrl,
    	    			dataType:"json",
    	    			success:function(responseID){
    	    			var matchId= responseID.matchID;    	    			
    	    			worldCupMatch=response.results.filter(function(item){return (item.matchdetail.series.id=="2995" && item.matchdetail.match.code==matchId);});
    	    			showsStickyLiveMatch(worldCupMatch);
    	    			}
    	    		});				
    	    	}
    			
    		}		
    	});
    	
    }    	
    	
    function showsStickyLiveMatch(worldCupMatch)
    {    	
    	var liveMatchDiv=""; 
    	var isDouble=false;
    	if(worldCupMatch.length>1){
    		isDouble=true;
    	}
    	if(worldCupMatch.length>0){
	        response=worldCupMatch;
	    	liveMatchDiv+="<div class='sticky-bottom'>";
	        liveMatchDiv+="<div class='stickyfooter'>";
	        liveMatchDiv+="<div class='container'>";
	        liveMatchDiv+="<div class='stickyslider'>";
	        liveMatchDiv+="<div class='swiper-wrapper'>";
	        	        
	        $.each(response,function(index, liveData){	
	    		if(index>1){return false;}
	    		var matchNo=liveData.matchdetail.match.number;
	    		var matchID=liveData.matchdetail.match.code;
	    		var teamA=liveData.teamlist[0].name_Short;
	    		var teamB=liveData.teamlist[1].name_Short;
	    		var teamFullA=liveData.teamlist[0].name_Full;
	    		var teamFullB=liveData.teamlist[1].name_Full;
	    		var equation=liveData.matchdetail.equation;
	    		var teamImgA="https://www.hindustantimes.com/images/app-images/cricket/world-cup/"+teamA+".svg";
	    		var teamImgB="https://www.hindustantimes.com/images/app-images/cricket/world-cup/"+teamB+".svg";
	    		var statusID=liveData.matchdetail.status_Id;
	    		var statusMsg, msgColor="";
	    		var team_Home = liveData.matchdetail.team_Home == liveData.teamlist[0].team_Id ? liveData.teamlist[0].name_Full : liveData.teamlist[1].name_Full;
	    		var team_Away = liveData.matchdetail.team_Away == liveData.teamlist[0].team_Id ? liveData.teamlist[0].name_Full : liveData.teamlist[1].name_Full;
	    		var team_Home_Slug = team_Home.toLowerCase().replace(/ /g,'-');
	    		var team_Away_Slug = team_Away.toLowerCase().replace(/ /g,'-');
	    		scoreCardURL="https://www.hindustantimes.com/cricket/live-scorecard-"+team_Home_Slug+"-vs-"+team_Away_Slug+"-live-score-"+matchID+"/";
	    		
	    		if(stopStatus.includes(statusID)){
	    			statusMsg="Play interrupted";
	    			msgColor="stopped-rain";
	    		}
	    		else if(statusAbandoned.includes(statusID)){
	    			statusMsg="Match Abandoned";
	    			msgColor="stopped-rain";
	    		}			
	    		else if(statusID=="110"){
	    			statusMsg="Innings Break";
	    		}
	    		else if(statusID=="118"){
	    			statusMsg="Play stopped due to rain";
	    			msgColor="stopped-rain";
	    		}
	    		else{
	    			statusMsg="";
	    		}   		
	    		
		        liveMatchDiv+="<div class='swiper-slide'>";
		        if(isDouble){
		        	liveMatchDiv+="<div class='flex-divs flex-100' onclick=\"sendURL('"+scoreCardURL+"')\">";
		        }else{
		        	liveMatchDiv+="<div class='flex-divs flex-pd-100' onclick=\"sendURL('"+scoreCardURL+"')\">";	
		        }
		        liveMatchDiv+="<div class='match-heading'>";
		        if(isLive){
			        liveMatchDiv+="<div class='match-live'>";
			        liveMatchDiv+="<i class='fa fa-circle'></i><span>LIVE</span>";
			        liveMatchDiv+="</div>";
		        }else{
			        liveMatchDiv+="<div class='result'>";
			        liveMatchDiv+="<span>RESULT</span>";
			        liveMatchDiv+="</div>";
		        }
		        liveMatchDiv+="<strong>ICC Cricket World Cup 2019, "+matchNo+"</strong>";
		        liveMatchDiv+="</div>";							
		        liveMatchDiv+="<ul class='team-centre flex-divs'>";
		        if(liveData.innings.length>0){	
			        liveMatchDiv+="<li class='team'>";
			        liveMatchDiv+="<div class='team-thumb'>";
			        liveMatchDiv+="<img src='"+teamImgA+"' alt='' title=''>";
			        liveMatchDiv+="</div>";
			        liveMatchDiv+="<div class='score'>";
			        liveMatchDiv+="<span class='team-name'>"+teamFullA+"</span>";
			        liveMatchDiv+="<strong>"+liveData.innings[0].total+"/"+liveData.innings[0].wickets+"</strong>";
			        liveMatchDiv+="<span class='mr-0'>"+liveData.innings[0].overs+" ovs</span>";
			        liveMatchDiv+="</div>";
			        liveMatchDiv+="</li>";
		        }
		        if(!isLive){
			   		liveMatchDiv+="<li>";
			        liveMatchDiv+="<div class='beat-team'>"+equation+"</div>";
			        liveMatchDiv+="</li>";
		        }
		        else{
		        if(statusMsg!=""){
	    			liveMatchDiv+="<li>";
	    			liveMatchDiv+="<div class='"+msgColor+"'>"+statusMsg+"</div>";
	    			liveMatchDiv+="</li>"; 
	    		}
	    		else{
	    			liveMatchDiv+="<li><div class='vs'>VS</div></li>"; 
	    		}
		        }
		      
		        if(liveData.innings.length>1){
			        liveMatchDiv+="<li class='team team-b'>";
			        liveMatchDiv+="<div class='team-thumb'>";
			        liveMatchDiv+="<img src='"+teamImgB+"' alt='' title=''>";
			        liveMatchDiv+="</div>";
			        liveMatchDiv+="<div class='score'>";
			        liveMatchDiv+="<span class='team-name'>"+teamFullB+"</span>";
			        liveMatchDiv+="<strong>"+liveData.innings[1].total+"/"+liveData.innings[1].wickets+"</strong>";
			        liveMatchDiv+="<span>"+liveData.innings[1].overs+" ovs</span>";
			        liveMatchDiv+="</div>";
			        liveMatchDiv+="</li>";
		        }
		        else{
		        	liveMatchDiv+="<li class='team team-b'>";
			        liveMatchDiv+="<div class='team-thumb'>";
			        liveMatchDiv+="<img src='"+teamImgB+"' alt='' title=''>";
			        liveMatchDiv+="</div>";
			        liveMatchDiv+="<div class='score'>";
			        liveMatchDiv+="<span class='team-name'>"+teamFullB+"</span>";
			        liveMatchDiv+="<strong class='mar-0'>Yet To Bat</strong>";
			        liveMatchDiv+="</div>";
			        liveMatchDiv+="</li>";
		        }
		        liveMatchDiv+="</ul>";
		        liveMatchDiv+="<a class='match-centre'>Match Centre</a>";
		        liveMatchDiv+="</div>";
		        liveMatchDiv+="</div>";
	        
	        });
	        
	        liveMatchDiv+="</div>";
	    	liveMatchDiv+="</div>";
	    	liveMatchDiv+="</div>";
	    	liveMatchDiv+="</div>";
	    	liveMatchDiv+="</div>";
	    	
	    	$("#sticky-match-card").html(liveMatchDiv);
        
    	}    	
    	
    }

    $(document).ready(function(){
    getStickyLiveScore();
    setInterval(function(){getStickyLiveScore(); }, 30000);
    	
    });
    
    function sendURL(url){
    	if(url==""){
    		window.location=scoreCardURL;
    	}
    	else{
    		window.location=url;
    	}
    	
    }
</script>
