<script>
var logincookie_token = getCookie("token");
var skippablePop = getCookie("skippablePop") || 'true';
var articleCount = getCookie("articleCount") || 0;
var intCount;
if (typeof setCookie == 'undefined') {
    function setCookie(e, t, a) {
        var i = new Date;
        i.setTime(i.getTime() + 24 * a * 60 * 60 * 1e3);
        var o = "expires=" + i.toGMTString();
        document.cookie = e + "=" + t + ";" + o + ";path=/";
    }
}
showSkippablePopup();

$('.closePop,.skipLink').click(function() {
    $('.skippablePop').hide();
    $('body').removeClass('stopScroll');
    setCookie("skippablePop", "false", 3);
    setCookie("articleCount", 0, 2);

});

$('.btnSignin').click(function() {
    $('.skippablePop').hide();
    $('body').removeClass('stopScroll');

});


function showSkippablePopup() {
	articleCount = getCookie("articleCount");
	intCount = articleCount;
    if (articleCount >= 3) {
        if (skippablePop == 'true' && !logincookie_token) {
            $('body').addClass('stopScroll');
            $('.skippablePop').css('display', 'flex');
        }
    } else {
        intCount++;
        setCookie("articleCount", intCount, 2);
    }

}
