var tmpArray = ["facebook", "twitter", "linkedin", "googleplus", "pinterest", "reddit", "whatsapp", "email", "telegram"];
for(i = 0; i < tmpArray.length; i++) { tmpCards.push(socialCards[tmpArray[i]]);
}
$.each(tmpCards, function(index){
var share = tmpCards[index];
share.url = url;
share.text = text;
_renderShareCount(share, _createSelector(share.faClass));
sUrl = _getShareUrl(share);
//share.shareUrl = sUrl;
var tmpKey = articleId + "-" + share.faClass;	articleShareData[tmpKey] = sUrl;
});
var share = socialCards.whatsapp;
var shareUrl = articleShareData[articleId + "-" + share.faClass];
shareUrl += window.encodeURIComponent("?utm_campaign=article_share&utm_medium=referral&utm_source=whatsapp.com");
$(data).find('.fa-whatsapp.wa').parent().attr("href", shareUrl)
.attr("target", "_" + share.shareIn);	share = socialCards.email;
shareUrl = articleShareData[articleId + "-" + share.faClass];
$(data).find('.fa-envelope.en').parent().attr("href", shareUrl)
.attr("target", "_" + share.shareIn);
});
});
