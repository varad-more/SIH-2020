    function displayAd($el, slotCode, sizes) {
        var elId = $el.attr('id');
        googletag.cmd.push(function () {
            var slot = googletag.defineSlot(slotCode, sizes, elId).addService(googletag.pubads()).setCollapseEmptyDiv(true,true);
            slot.addService(googletag.pubads());
            googletag.display(elId);
        });
    }
    
    function getPersonlizeData(elm) {
        var forYoudata = '';
        var count = 1;
        var gaid = '';
        var callApi = false;
        var filterData, forutxt, foruContent;
        var gaexp = getCookie("_gaexp");
        if (elm !== '') {
            forutxt = $(elm).find('#forutxt');
            foruContent = $(elm).find('#foru-content');
        } else {
            forutxt = "#forutxt";
            foruContent = "#foru-content";

        }
        if (gaexp && userCookie) {
        	var expId=gaexp.split('.');
        	expId=expId.slice(2,expId.length);
        	gaexp=expId.join('.');
            var gaStr = gaexp.split('!');
            gaStr.forEach(function(val) {
                var x = val.split('.')[0];
                if (x == abStr) {
                    gaid = val.split('.').pop();
                    if (gaid == 0 || gaid == 1) {
                        callApi = true;
                    }
                    else if( gaid == 2){
                     $(forutxt).html('Recommended for you');	
                    }

                }
            });
        }
        if (callApi && forYouApiResponse=='') {
            $.ajax({
                url: "https://www.hindustantimes.com/personalized-info?user-cookie=" + userCookie + "&gaexp=" + gaid+"&limit=15&storyid="+storyId,
                type: "GET",
                dataType: "json",
                success: function(res) {
                    if (res.stories.length > 4) {
                        filterData = res.stories.filter(function(data){return data.isContent;});
                        $(filterData).each(function(i, item) {
                            forYoudata += "<div class='col-sm-6 col-md-4 col-lg-3'>";
                            forYoudata += "<div class='mb-30 top-news-design'>";
                            forYoudata += "<div class='relative-icons'>";
                            forYoudata += "<a href='" + siteBaseUrl + item.url + "' data-id='for-you," + count + "'>";
                            forYoudata += "<img loading='lazy' src='https://www.hindustantimes.com" + item.image_444x250 + "' title='" + item.title + "' alt='" + item.title + "'>";
                            forYoudata += "</a>";

                            forYoudata += "</div>";
                            forYoudata += "<div class='headingfour'>";
                            forYoudata += "<a href='" + item.url + "' data-id='for-you," + count + "' class='wclink2'>";
                            forYoudata += item.title + "</a>";
                            forYoudata += "</div>";

                            forYoudata += "</div>";
                            forYoudata += "</div>";
                            count++;
                            if (i === 7) {
                                return false;
                            }
                        });
                        forYouApiResponse=forYoudata;
                        $(forutxt).html('Recommended for you');
                        $(foruContent).html(forYoudata);
                    }
                }
            });
        }
        else if(forYouApiResponse!=''){
        	$(forutxt).html('Recommended for you');
            $(foruContent).html(forYouApiResponse);
        }
    }
    
