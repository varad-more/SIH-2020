	function ScrollMe(uuid) {
		var id = uuid.replace('story_','').trim();
	    id = id +"_story";
		var newid = uuid.trim();
		$('#ulInfinite').each(function() {
			var phrase = '';
			$(this).find('li').each(function(j, lix) {
				var myid = $(lix).attr('id');
				if (myid.trim() == newid.trim())
					$(lix).addClass("active");
				else
					$(lix).removeClass("active");
			});
		});

		var element = document.getElementById(id);		
		element.scrollIntoView();
		element.scrollIntoView({
			behavior : "auto",
			block : "start",
			inline : "nearest"
		});

		$('html, body').animate({
			scrollTop : $("#" + newid).offset().top - 800
		}, 800, 'swing');
		
	}
