	$.fn.isInViewport = function () {
		if($(this).length>0){
        var elementTop = $(this).offset().top - preViewportDistance;
        var elementBottom = elementTop + $(this).outerHeight() + preViewportDistance;
        var viewportTop = $(window).scrollTop();
        var viewportBottom = viewportTop + $(window).height();
        return elementBottom > viewportTop && elementTop < viewportBottom;
		}else{
			return false;
		}
    };
    
    function getCookie(e) {
        for (var a = e + "=", t = document.cookie.split(";"), n = 0; n < t.length; n++) {
            for (var o = t[n];
                " " == o.charAt(0);) o = o.substring(1, o.length);
            if (0 == o.indexOf(a)) return o.substring(a.length, o.length)
        }
        return null
    }

