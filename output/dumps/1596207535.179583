				window.open('mailto:?subject='+sub+'&body='+body);
			}else if(type=='prev'){
				window.location.href= mb_prev;
			}else if(type=='next'){
				if(mb_next!=undefined && mb_next!=''){
					window.location.href= mb_next;
				}
			}else{
				$('a[data-type="'+type+'"]').click();
			}
		}
	</script>
</div>

