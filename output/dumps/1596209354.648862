<script language="javascript" type="text/javascript">

    $(document).ready(function () {
        //if($(window).width()<767)
        //{
        //alert();
        //    $(".main_ul").lightSlider({
        //		item:1,
        //        loop:true,
        //        keyPress:true,
        //		slideMove:1,
        //	
        //    });
        //}

        // JavaScript Document
        var a = parseInt($(".clk_count").html());
        var li_wid;
        var $rgt_btn = $(".more_cors_btn_wpr").find("#righty"),
		$lft_btn = $(".more_cors_btn_wpr").find("#lefty"),
		li_count = $(".grid li").length,
		preview = $(".course_wpr").width();
        //alert(li_count);

        function move_course() {

            $(".more_cors_btn_wpr").find("#righty").addClass("righty");
            if ($lft_btn.hasClass("lefty")) {
                //alert("ahe");
                $lft_btn.removeClass("lefty");
            }

            if ($(window).width() < 768) {

                li_wid = parseInt($(".course_ul li").outerWidth()),
		extrem_cnt = li_count - 1;
            }
            else if ($(window).width() > 767 && $(window).width() < 992) {
                li_wid = parseInt($(".course_ul li").outerWidth()) + 30,
		extrem_cnt = li_count - 5;
            }
            else if ($(window).width() > 991 && $(window).width() < 1200) {
                li_wid = parseInt($(".course_ul li").outerWidth()) + 30,
		extrem_cnt = li_count - 7;
            }
            else {
                li_wid = parseInt($(".course_ul li").outerWidth()) + 30,
		extrem_cnt = li_count - 8;
            }


            //alert(extrem_cnt);		
            function MoveLeft() {
                $(".course_ul").stop(true, true).animate({ "marginLeft": "-=" + li_wid });
            }
            function MoveRight() {
                $(".course_ul").stop(true, true).animate({ "marginLeft": "+=" + li_wid });
            }

            function rght_clk() {
                $rgt_btn.click(function () {
                    if ($(this).hasClass("righty")) {
                        debugger;
                        $lft_btn.removeClass("low_opacity_lft").addClass("lefty");
                        MoveLeft();

                        a = a + 1;
                        $(".clk_count").html(a);
                        if (a == extrem_cnt) {
                            $(this).addClass("low_opacity_rgtt").removeClass("righty");
                        }
                    }
                })
            }
            rght_clk();
            function lft_clk() {
                $lft_btn.click(function () {
                    if ($(this).hasClass("lefty")) {
                        $rgt_btn.removeClass("low_opacity_rgtt").addClass("righty");
                        MoveRight();
                        a = a - 1;
                        $(".clk_count").html(a);
                        if (a == 0) {
                            $(this).addClass("low_opacity_lft").removeClass("lefty");
                        }
                    }

                })
            }
            lft_clk();
        }
        move_course();
        $(window).resize(function () {
            if ($(window).width() > 768) {
                $(".course_ul").css("margin-left", 0);
                $(".clk_count").html("0");
                a = 0;
                $(".more_cors_btn_wpr").find("#righty").addClass("righty").removeClass("low_opacity_rgtt");
                $(".more_cors_btn_wpr").find("#lefty").addClass("low_opacity_lft").removeClass("lefty");

                if ($(window).width() < 768) {
                    li_wid = parseInt($(".course_ul li").outerWidth()),
			extrem_cnt = li_count - 1;
                }
                else if ($(window).width() > 767 && $(window).width() < 992) {
                    li_wid = 372,
			extrem_cnt = li_count - 2;
                }
                else {
                    li_wid = parseInt($(".course_ul li").outerWidth()) + 30,
			extrem_cnt = li_count - 3;
                }

                rght_clk();
                lft_clk();
            }
        })
    });
</script>
