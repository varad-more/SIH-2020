    function trimall(sString) {
        while (sString.substring(0, 1) == " ") {
            sString = sString.substring(1, sString.length);
        }
        while (sString.substring(sString.length - 1, sString.length) == ' ') {
            sString = sString.substring(0, sString.length - 1);
        }
        return sString;
    }
    function ShowImage(evt) {
        debugger;
        if (trimall(document.getElementById('ContentPlaceHolder1_ucCompanyInfoTop1_txtCompanySearch').value) == "")
            document.getElementById("divCompanySearchLoading1").style.display = "none";
        else {
            if (document.getElementById('ContentPlaceHolder1_ucCompanyInfoTop1_txtCompanySearch').value.length >= 2)
                document.getElementById("divCompanySearchLoading1").style.display = "inline";
            else
                document.getElementById("divCompanySearchLoading1").style.display = "none";
        }
    }
    function isSpeCharComp(evt) {
        if (evt.keyCode != 32 && evt.keyCode != 46) {
            var charCode = (evt.which) ? evt.which : event.keyCode
            if ((charCode > 31 && charCode < 48) || (charCode > 57 && charCode < 65) || (charCode > 90 && charCode < 97) || (charCode > 122 && charCode <= 127)) {
                document.getElementById('ContentPlaceHolder1_ucCompanyInfoTop1_txtCompanySearch').focus();
                return false;
            }
        }
        return true;
    }
    function validate() {
        var txtSearch = document.getElementById('ContentPlaceHolder1_ucCompanyInfoTop1_txtCompanySearch').value;
