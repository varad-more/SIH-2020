                var PrevClose = Datastring[1];
                var chart = AmCharts.makeChart('divCommodityChartNew', { 'type': 'serial', 'theme': 'light', 'paddingRight': 0, 'color': '#fff', 'fontfamily': 'Roboto Condensed',
                    'pathToImages': '/images/',
                    'dataProvider': chartData,
                    'valueAxes': [{ 'id': 'v1',
                        'axisColor': '#FF6600',
                        'axisThickness': 2,
                        'gridAlpha': 0,
                        'axisAlpha': 1,
                        'position': 'left'
                    }],
