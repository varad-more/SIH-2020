    $(".drp_option").each(function (i) {
        $(this).attr('id', 'dp_' + i);
        var a = $(this).attr("id");
        var $elem = $("#" + a);
        var $dp_dwn = $elem.find(".drp_dwnP")
        $dp_dwn.click(function () {
            $(this).next("ul").slideToggle();
            var $par_elem = $(this).parentUntil(".drp_option");
            $par_elem.find(".close_box").toggle();
            $(".drp_dwnP").not(this).next("ul").slideUp();
            $(".drp_dwnP").not(this).next("ul").next(".close_box").hide();
        })
        var $li = $elem.find("li");
        $li.click(function () {
            var prnt = $(this).parent("ul").prev(".drp_dwnP");
            var txt = $(this).html();
            if (txt.indexOf('52 Week H/L') >= 0) {
                $(".52WK_HL").show();
                $(".todayHighLow").hide();
                $(".PE").hide();
                $(".PB").hide();
                $(".divyield").hide();
            }
            else if (txt.indexOf('Todays H/L') >= 0) {
                $(".52WK_HL").hide();
                $(".todayHighLow").show();
                $(".PE").hide();
                $(".PB").hide();
                $(".divyield").hide();
            }
            else if (txt.indexOf('PE') >= 0) {
                $(".52WK_HL").hide();
                $(".todayHighLow").hide();
                $(".PE").show();
                $(".PB").hide();
                $(".divyield").hide();
            }
            else if (txt.indexOf('PB') >= 0) {
                $(".52WK_HL").hide();
                $(".todayHighLow").hide();
                $(".PE").hide();
                $(".PB").show();
                $(".divyield").hide();
            }
            else if (txt.indexOf('Dividend Yield(%)') >= 0) {
                $(".52WK_HL").hide();
                $(".todayHighLow").hide();
                $(".PE").hide();
                $(".PB").hide();
                $(".divyield").show();
            }
            prnt.find(".siz_txt").html(txt);
            $(this).parent("ul").slideUp();
        })
    });


	
