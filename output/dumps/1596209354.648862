                    'valueAxis': 'v1',
                    'lineColor': '#48B647',
                    'fillColorsField': '#D3575E',
                    'bullet': 'round',
                    'autoGridCount': false,
                    'gridCount': 3,
                    'bulletBorderThickness': 0,
                    'hideBulletsCount': 5,
                    'valueField': 'value',
                    'title': '', 'fillAlphas': 0.2,
                    'negativeLineColor': "#E32E32",
