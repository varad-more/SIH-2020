                        				innerScopeWidth = innerScope.outerWidth(true),
                        				totalLeft = liLeft + innerScopeWidth,
                        				marginLeftVal = parseInt(scope.find('ul.tUl').css("marginLeft"));
                        				innerScope.addClass('active').siblings().removeClass('active');				
                        				if(liLeft < 0){
                        					marginLeftVal = marginLeftVal - liLeft;
                        				}else if(totalLeft > viewWidth){
                        					marginLeftVal = viewWidth - (totalLeft + Math.abs(marginLeftVal));
                        				}
                        				btnSH(getIndex);
                        				tabAnimate(marginLeftVal);				
                        				loadSlideContent();
                        			}				
                        		});
                        		
                        		btnScope.off().on('click', function(){
                        			if(flag && !$(this).hasClass('disabled')){
                        			    scope.parent().find('.sld_loading').show();
                        				flag = false;
                        				var innerScope = $(this),
                        				viewWidth = scope.outerWidth(true),				
                        				getAlt = $(this).attr('alt'),
                        				getIndex = scope.find('ul.tUl li.active').index(),
                        				nextIndex = 0;
                        				switch(getAlt){
                        					case 'left':
                        						nextIndex = getIndex > 0 ? getIndex - 1 : 0;
                        						break;
                        					case 'right':
                        						nextIndex = (getIndex + 1 == liLength) ? getIndex  : getIndex + 1;
                        						break;	
                        				}				
                        				scope.find('ul.tUl li:eq('+ nextIndex +')').addClass('active').siblings().removeClass('active');
                        				btnSH(scope.find('ul.tUl li.active').index());				
                        				var liLeft = scope.find('ul.tUl li.active').offset().left + (scope.find('ul.tUl li.active').outerWidth(true) / 2) + Math.abs(parseInt(scope.find('ul.tUl').css("marginLeft")));
                        				slideleft = viewWidth - liLeft;
                        				slideleft = slideleft > -1 ? 0 : slideleft;		
                        				tabAnimate(slideleft);			
                        				loadSlideContent();
                        			}									
                        		});
                        		
                        		init();
                        		
                        	});
                        }
                        
                        $.fn.contentSlide = function(){
                        	return this.each(function(){
                        		var scope = $(this),
                        		slideLength = scope.find('.slideDiv').length,
                        		slideWidth = scope.find('.slideDiv').outerWidth(true),
                        		totalWidth = slideLength * slideWidth,
                        		sliderBtn = scope.find('.slideBTN'),
                        		nextBtn = scope.find('.indRight'),
                        		flag = true,
                        		slideEffect = function(marginVal, sliderCount){
                        			scope.find('.sliderContainer').animate({
                        				marginLeft : '-'+ marginVal +'px'
                        			}, 500, function(){
                        				flag = true;
                        				scope.parent().attr('data-current-slide', sliderCount)
                        			});
                        		},
                        		currentVisibleMsid = function() {
                        			if(scope.parents('.content').prev().find('.tUl li').hasClass('active')){
                        				return scope.parents('.content').prev().find('.tUl li.active').attr('data-msid');
                        			} else {
                        				return scope.parents('.content').prev().find('.tUl li').eq(0).attr('data-msid');
                        			}
                        		},
                        		load = function (msid) {
                        			scope.parents('.sliderContent').find('.sld_loading').show();
                        			var my_obj = scope.parents('.content').prev().find('[data-msid="' + msid + '"]');
                        			var curpg = parseInt(my_obj.attr('data-curpg')) + 1;
                        			my_obj.attr('data-curpg', curpg)
                        			$.ajax({
                        				url: my_obj.attr('data-url') + '&partial=1&curpg=' + curpg,
                        				success:function (data) {
                        					var slideDivCount = (data.match(new RegExp('class="slideDiv"', "g")) || []).length;
                        					scope.parent().find('.sliderContainer').append(data).promise().done(function(){
                        					    
                        						if(slideDivCount == 0){
                        						    var sliderCount = parseInt(scope.parent().attr('data-current-slide')) - 1;
                        							var marginVal = sliderCount * slideWidth;
                        							slideEffect(marginVal, sliderCount);
                        							scope.find('.indRight').addClass('disabled').promise().done(function(){
                        							    scope.parent().attr('data-current-slide', sliderCount)
                        							});
                        						}else{
                        						    scope.find('.indRight').removeClass('disabled');
                        						}
                        					});
                        					scope.parents('.sliderContent').find('.sld_loading').hide();				
                        				},
                        				complete: function () {
                        					scope.parents('.sliderContent').find('.sld_loading').hide();
                        				}
                        			});
                        		}, 
                            	btnSH = function(sliderCount){
                        			//console.log(sliderCount +"=="+ slideLength)
                        			var newLength = slideLength - 1
                        			if(sliderCount == 0){
                        				scope.find('.indRight').removeClass('disabled');
                        				scope.find('.indleft').addClass('disabled');
                        			}else if(sliderCount == slideLength || sliderCount == newLength){
                        				scope.find('.indleft').removeClass('disabled');
                        				scope.find('.indRight').addClass('disabled');					
                        			}else{
                        				scope.find('.slideBTN').removeClass('disabled');		
                        			}
                        		},
                        		init = function(){			
                        			scope.find('.sliderContainer').width(totalWidth);
                        			if(!scope.parents('.sliderContent').children().hasClass('sld_loading')){
                        				scope.parents('.sliderContent').append("<div class='sld_loading'><div><img src='"+objDomain.img+"/photo/20259450.cms' alt='Loading'><br/>Loading</div></div>");
                        			}
                        			var sliderCount = scope.parent().attr('data-current-slide');
                        			sliderCount = typeof sliderCount == 'undefined' ? 0 : sliderCount;
                        			btnSH(sliderCount);
                        		};	
                        		
                        		sliderBtn.off().on('click', function(){
                        			if(flag && !$(this).hasClass('disabled')){
                        				flag = false;
                        				var btnScope = $(this),
                        				getAlt = btnScope.attr('alt'),
                        				getIndex = scope.find('.slideDiv.active').index(),
                        				current_visible_msid = currentVisibleMsid(),
                        				sliderCount = scope.parent().attr('data-current-slide'),
                        				sliderCount = typeof sliderCount == 'undefined' ? 0 : sliderCount;
                        				switch(getAlt){
                        					case 'prev':
                        					    
                        						if (sliderCount > 0) {
                        							sliderCount --;
                        							var marginVal = sliderCount * slideWidth;
                        							slideEffect(marginVal, sliderCount);
                        						}
                        						break;
                        					case 'next':
                        					    
                        						sliderCount++;
                        						var marginVal = sliderCount * slideWidth;
                        						slideEffect(marginVal, sliderCount);
                        						var curpg = scope.parents('.content').prev().find('.tUl li[data-msid="' + current_visible_msid + '"]').attr('data-curpg');
                        						if(sliderCount == slideLength) {
                        							load(current_visible_msid);
                        						}
                        						break;	
                        				}
                        				
                        				btnSH(sliderCount);
                        				
                        			}
                        		});
                        		init();
                        	});
                        }
                    	$('.sliderContent .tabs').tabSlide();
                    	$('.sliderContent .sliderLi_ctn .sliderImg_ctn').contentSlide();	
                    })(jQuery);
				
					if(!$(".eph").hasClass("initiated")){
        				$(".eph").addClass("initiated").etSlider({
        					visible : 3,
        					perSlide : 3,
        					slides : '#epslideshow > ul > li',
        					pagination : ".pagination",
        					slideContainer : '#epslideshow',
        					interval : 15000
        				});
        			}
                    
					
				</script></body></html>