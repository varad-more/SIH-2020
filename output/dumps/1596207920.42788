function getCookieWe(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(";");
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == " ") {
            c = c.substring(1, c.length);
        }
        if (c.indexOf(nameEQ) == 0) {
            return c.substring(nameEQ.length, c.length);
        }
    }
    return null;
}
function we() {
//var url = new URL(window.location.href);
if(window.location.href.indexOf(".html") != -1) {
 var viewData = {};
 	var cookie_token = getCookieWe("token"); 
	var user_name = getCookieWe("name");
	viewData["user state"] = "Anonymous"
	if(cookie_token && user_name){
		viewData["user state"] = "Signed In"
	}
 	
                viewData["section"] = "Insurance";
           viewData["subsection"] = "News";
		    viewData["publishedDate"] = "10 Jun 2020";
		      viewData["publishedTime"] = "09:58 PM IST"; 
		     viewData["author"] = "Anirudh Laskar"; 
		     viewData["storyType"] = "story"; 
		      viewData["timeToRead"] = parseInt("3");
             viewData["articleDesk"] ="DIGITAL";
     console.log(viewData);	
     	
     webengage.track("ArticleRead",  viewData);

  window.onbeforeunload = function (event) {
     
     var viewData = {};
           viewData["section"] = "Insurance";
           viewData["subsection"] = "News";
		     viewData["storyType"] = "story"; 
              console.log(viewData);
           	webengage.track("BrowserClose", viewData); 

};

} else {
}

 

} 
we();
