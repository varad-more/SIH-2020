<script type="text/javascript">


    app.controller("calcontroller", function ($scope, $timeout, $http) {
        //  alert();
        $scope.bindeventcalender = function (duration) {
          //  alert(duration);
            $scope.getclass = duration;
            sessionStorage.setItem("duration", duration);
            $http.post('/Dashboard.aspx/bindeventcalender', {
                duration: duration
            })
            .success(function (data, status, headers, config) {
                var output = data.d.split('|');
              //  alert(output);
               // debugger;
                $("#idresults").html(output[0]);
                $("#idBoard").html(output[1]);
                $("#idDividends").html(output[2]);
                $("#idAGM").html(output[3]);
                $("#idEGM").html(output[4]);
                $("#idSplits").html(output[5]);
                $("#idBonus").html(output[6]);
                $("#idRights").html(output[7]);
            });

        };
        $scope.redirectcalender = function (option) {
            sessionStorage.setItem("opt", option);
            $("#openpopupnew").show();
            $("#bindshhow").show();
            $("#bindhide").hide();
            $scope.bintevencalendar();
            

        };
        $scope.bintevencalendar = function () {
               
                var period = sessionStorage.getItem("duration")
                option = sessionStorage.getItem("opt")
              //  alert(option);

                $scope.classfile = period;
              //  alert(period + " | " + option);
                if (option == null) {
                    //window.location = '/';
                   
                }
                else {
                    $scope.getoption = option;
                    $scope.Bindcalender(period, option);
                }

            };


            $scope.Bindcalender = function (prd, opt) {
                //alert(prd);
               // alert(opt);
                $scope.loadingImage = true;
           //    debugger;
                if (option == "QR") {
                    $scope.pagehead = "Results Announcement";
                    $scope.tablehead = "Result Date";
                }
                else if (option == "BM") {
                    $scope.pagehead = "Board Meetings";
                    $scope.tablehead = "Board-Meeting Date";
                }
                else if (option == "D") {
                    $scope.pagehead = "Dividends";
                    $scope.tablehead = "Dividend Date";
                }
                else if (option == "AGM") {
                    $scope.pagehead = "AGM";
                    $scope.tablehead = "AGM Date";
                }
                else if (option == "EGM") {
                    $scope.pagehead = "EGM";
                    $scope.tablehead = "EGM Date";
                }
                else if (option == "S") {
                    $scope.pagehead = "Splits";
                    $scope.tablehead = "Split Date"
                    ;
                }
                else if (option == "B") {
                    $scope.pagehead = "Bonus";
                    $scope.tablehead = "Bonus Date";
                }
                else if (option == "R") {
                    $scope.pagehead = "Rights";
                    $scope.tablehead = "Rights Date";
                }
                $http({
                    method: 'POST',
                    url: '/EventCalender.aspx/evencal',
                    data: "{period: '" + prd + "',option: '" + option + "'}",
                }).then(function success(response) {
                    
                   // debugger;
                    $scope.loadingImage = false;
                    var output = response.data.d;
                    //alert(output);
                    if(output != "nodata")
                    {
                        $scope.bindeventcal = JSON.parse(output);
                    }
                    else
                    {
                        $("#bindshhow").hide();
                        $("#bindhide").show();
                    }
                    
                    
                    $scope.setPage = function (pageNo) {
                        $scope.currentPage = pageNo;
                    };
                    $scope.pageChanged = function () {
                        console.log('Page changed to: ' + $scope.currentPage);
                    };
                    $scope.setItemsPerPage = function (num) {
                        $scope.itemsPerPage = num;
                        $scope.currentPage = 1; //reset to first page
                    };
                },
                function error(response) {

                });
            }
    });
</script>
