<!-- NEWSLETTER -->
<script>
jQuery(document).ready(function() {
	jQuery("#signup-form").submit(function(e){
		e.preventDefault(); 
		
		var jQueryform = jQuery(this),
		name = jQueryform.find('input[name="name"]').val(),
		email = jQueryform.find('input[name="email"]').val(),
		url = jQueryform.attr('action');
		
		jQuery.post(url, {name:name, email:email},
		  function(data) {
			  if(data)
			  { 
				if(data=="Some fields are missing.")
				{
					jQuery("#newsletter_status").text("Please fill your name and email.");
					jQuery("#newsletter_status").css("color", "red");
				}
				else if(data=="Invalid email address.")
				{
					jQuery("#newsletter_status").text("Your email address is invalid.");
					jQuery("#newsletter_status").css("color", "red");
				}
				else if(data=="Invalid list ID.")
				{
					jQuery("#newsletter_status").text("Your list ID is invalid.");
					jQuery("#newsletter_status").css("color", "red");
				}
				else if(data=="Already subscribed.")
				{	//jQuery("#signup-form")[0].reset();
					jQuery("#newsletter_status").text("You're already subscribed!");
					jQuery("#newsletter_status").css("color", "red");
				}
				else
				{
					jQuery("#newsletter_status").text("You're subscribed!");
					jQuery("#newsletter_status").css("color", "green");
				}
			  }
			  else
			  {
				alert("Sorry, unable to subscribe. Please try again later!");
			  }
		  }
		);
	});
	jQuery("#signup-form").keypress(function(e) {
			if(e.keyCode == 13) {
				e.preventDefault(); 
				jQuery(this).submit();
			}
		});
	jQuery("#submit-btn").click(function(e){
		e.preventDefault(); 
		jQuery("#signup-form").submit();
	});
});

 var iframes; 
  var loadIframes = function(){
	iframes = document.getElementsByClassName('custom-iframe');
	for(var i=0; i<iframes.length; i++){

	  var iframe = iframes[i],
		  seconds = iframe.getAttribute('data-seconds'),
		  src = iframe.getAttribute('data-src');

	  run(i, src, seconds);
	}
  }

  var run = function(i, src, seconds){
	setTimeout(function(){
	  iframes[i].src = src;
	}, seconds);
  }

jQuery(document).ready(function(){
  loadIframes();
});
</script>
