        $(document).ready(function () {
            //Load comments when the page is loaded
            fnRefreshUserComments();

            if (window.location.pathname == "/account/login") {
                $('.sidebarCommentary').addClass('closed');
            }

            $('a[href="' + this.location.pathname + '"]').parent().addClass('active');
            $('a[href="' + this.location.pathname + '"]').parent().parent().parent().addClass('selected');
            $('a[href="' + this.location.pathname + '"]').parent().parent().parent().children('a').children().removeClass('down')
            $('a[href="' + this.location.pathname + '"]').parent().parent().parent().children('a').children().addClass('up')

            //Title Text
            if ($('#spnWatchListName').length) {
                $('#spnWatchListName').html($('a[href="' + this.location.pathname + '"]')[0].innerHTML);
            }

            $(".cd-top-nav li").removeClass("selected");
            $(".cd-top-nav li a img").removeClass("up");
            $(".cd-top-nav li a img").removeClass("down");

            $('#sidebarCollapse').on('click', function () {
                $('#sidebar').toggleClass('active');
            });

            $('#carousel-pager .vertical .item').each(function () {
                var next = $(this).next();
                if (!next.length) {
                    next = $(this).siblings(':first');
                }
                next.children(':first-child').clone().appendTo($(this));

                for (var i = 1; i < 8; i++) {
                    next = next.next();
                    if (!next.length) {
                        next = $(this).siblings(':first');
                    }

                    next.children(':first-child').clone().appendTo($(this));
                }
            });

